/dev/mapper/internvg-root /                       ext4    defaults        1 1
UUID=ed26b473-d518-4a67-8063-eec76ae9c084 /boot                   ext4    defaults        1 2
/dev/mapper/internvg-opt /opt                    ext4    defaults        1 2
/dev/mapper/internvg-tmp /tmp                    ext4    defaults        1 2
/dev/mapper/internvg-usr /usr                    ext4    defaults        1 2
/dev/mapper/internvg-var /var                    ext4    defaults        1 2
/dev/mapper/internvg-swap swap                    swap    defaults        0 0
tmpfs                   /dev/shm                tmpfs   defaults        0 0
devpts                  /dev/pts                devpts  gid=5,mode=620  0 0
sysfs                   /sys                    sysfs   defaults        0 0
proc                    /proc                   proc    defaults        0 0
